<p id="notice"><%= notice %></p>

<h1>Favorite Stocks</h1>

<table>
  <thead>
    <tr>
      <!-- <th>User</th> -->
      <th>Symbol</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @favorite_stocks.each do |favorite_stock| %>
      <tr>
        <!-- <td><%= favorite_stock.user_id %></td> -->
        <td><%= favorite_stock.symbol %></td>
        <td><%= link_to ' Show ', favorite_stock %></td>
        <td><%= link_to ' Edit ', edit_favorite_stock_path(favorite_stock) %></td>
        <td><%= link_to ' Delete ', favorite_stock, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Favorite Stock', new_favorite_stock_path %>
